{
  "compilerOptions": {   
    "baseUrl": "./src/",
    "target": "es6",
    "sourceMap": true,
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "incremental": true
  },
  "exclude":[
    "./node_modules"
  ]
}